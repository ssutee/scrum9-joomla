	<params>
		<param name="showtools" type="radio" default="1" label="Show Tools of template" description="">
			<option value="1">Show</option>
			<option value="0">Hide</option>
		</param>
		<param name="jv_font" type="list" default="4" label="Font size" description="Select Font size for your site">
			<option value="1">1</option>
			<option value="2">2</option>
			<option value="3">3</option>
			<option value="4">4</option>
			<option value="5">5</option>
			<option value="6">6</option>
		</param>
		<param name="jv_layout" type="list" default="lcr" label="Layout" description="Select Layout for your site">
			<option value="lcr">Left - Content - Righ</option>
			<option value="clr">Content - Left - Right</option>
		</param>
		<param name="jv_color" type="list" default="green" label="Color Variation" description="Color variation to use">
			<option value="green">Green</option>
			<option value="blue">Blue</option>
			<option value="red">Red</option>
		</param>
		<param name="menutype" type="menu" default="mainmenu" label="Menu Type" description="Menu to show on main navigation" />
			<param name="jv_menustyle" type="list" default="moo" label="Menu Module" description="MENU'S TYPE">
			<option value="none">Disable</option>
			<option value="moo">MooMenu</option>
		
		</param>
		<param name="spacer@" type="spacer" />
		<param name="gzip" type="radio" default="0" label="Gzip Compression" description="Do you want to enable gzip compression for Javascript and Css (Some host dont have gzip module enabled, If see that your site is not loading css and js please disable this parameter.">
			<option value="0">No</option>
			<option value="1">Yes</option>
		</param>
	</params>

</install>